{
  "name": "codenotes-monorepo",
  "version": "1.0.0",
  "description": "Code Notes AI-Powered Notebook",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "devDB": "pscale connect code-snips dev",
    "dev": "concurrently \"npm run client\" \"npm run server\" \"npm run devDB\"",
    "build": "concurrently \"cd client && npm run build\" \"cd server && npm run build\"",
    "start": "cd server && npm run start"
  },
  "author": "Tim Dronkers",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
